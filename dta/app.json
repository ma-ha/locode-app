      },
      "Customer": {
        "title": "Customer",
        "scope": "inherit",
        "maintainer": [
          "appUser"
        ],
        "properties": {}
      }
    },
    "startPage": [
      "DeviceOrder",
      "Invoice",
      "Customer"
    ],
    "role": [
      "appUser"
    ],
    "scope": {
      "#region": {
        "role": [
          "appUser"
        ],
        "srcURL": ""
      },
      "#city": {
        "role": [
          "appUser"
        ],
        "srcURL": ""
      }
    }
  },
  "1000/device-mgr-app/1.0.0": {
    "type": "dev/locode-entity-editor/0.1.0",
    "scopeId": null,
    "title": "Device Manager",
    "require": {},
    "entity": {
      "Device": {
        "title": "IoT Device",
        "scope": "no-inherit",
        "maintainer": [
          "appUser"
        ],
        "properties": {
          "DeviceNo": {
            "type": "String",
            "filter": true
          },
          "Status": {
            "type": "Select",
            "filter": true,
            "options": [
              "Ordered",
              "Inventory",
              "Planned",
              "Active",
              "Defect",
              "Decommissioned"
            ]
          },
          "Hardware": {
            "type": "SelectRef",
            "filter": true,
            "selectRef": "1000/device-mgr-app/1.0.0/DeviceHardware"
          },
          "Description": {
            "type": "String"
          },
          "ConnType": {
            "label": "In Data Link",
            "type": "Select",
            "filter": true,
            "options": [
              "COAP",
              "HTTP/S",
              "LW-M2M",
              "LORA",
              "MQTT",
              "UDP"
            ]
          },
          "Tags": {
            "label": "Out Data Link",
            "type": "MultiSelectRef",
            "multiSelectRef": "1000/device-mgr-app/1.0.0/DeviceTags"
          }
        }
      },
      "DeviceHardware": {
        "title": "IoT Hardware",
        "scope": "inherit",
        "maintainer": [
          "appUser"
        ],
        "properties": {
          "Manufacturer": {
            "type": "String"
          },
          "Name": {
            "type": "String"
          },
          "Revision": {
            "type": "String"
          },
          "LinkType": {
            "type": "Select",
            "options": [
              "BT-LE",
              "LAN",
              "LTE",
              "LTE-M",
              "LORA",
              "NB-IoT",
              "MODBUS",
              "...other"
            ]
          },
          "LinkToSAP": {
            "label": "Open in SAP",
            "type": "Link",
            "link": "https://localhost/sap/customer/${id}"
          }
        }
      },
      "DataLink": {
        "title": "IoT Data Link",
        "scope": "inherit",
        "maintainer": [
          "appUser"
        ],
        "properties": {
          "LinkType": {
            "type": "Select",
            "options": [
              "WebHook",
              "Azure Event Hub",
              "AWS IoT Central",
              "CoT"
            ]
          },
          "Comment": {
            "type": "String"
          },
          "ConnectionString": {
            "type": "String"
          },
          "TrafficStats": {
            "type": "Metric",
            "apiManaged": true
          },
          "DeviceStats": {
            "type": "Metric",
            "apiManaged": true
          },
          "DeviceTags": {
            "type": "MultiSelectRef",
            "multiSelectRef": "1000/device-mgr-app/1.0.0/DeviceTags"
          }
        }
      },
      "DeviceTags": {
        "title": "Device Tags",
        "scope": "inherit",
        "maintainer": [
          "appUser"
        ],
        "properties": {
          "Tag": {
            "type": "String"
          },
          "Description": {
            "type": "String"
          }
        }
      }
    },
    "startPage": [
      "Device",
      "DataLink",
      "DeviceHardware",
      "DeviceTags"
    ],
    "role": [
      "appUser"
    ],
    "scope": {
      "#region": {
        "role": [
          "appUser"
        ],
        "srcURL": ""
      },
      "#city": {
        "role": [
          "appUser"
        ],
        "srcURL": ""
      }
    }
  },
